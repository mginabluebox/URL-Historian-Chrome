{
  "name": "URL Historian",
  "version": "1.0.8",
  "description": "URL Historian - Enlightening the social impact of the web through your web use",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "permissions": ["activeTab","tabs","storage","background","alarms"],
  "icons" : {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": ["jquery/jquery-3.5.1.min.js","jquery/jquery-ui.min.js",
                "tz/moment-with-locales.min.js","tz/moment-timezone-with-data.min.js",
                "tz/select2.min.js",
                "aws-sdk.min.js","background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
     "16": "images/icon16.png",
     "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
